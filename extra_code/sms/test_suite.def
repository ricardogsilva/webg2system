suite suiteTeste
    edit primeiro Ricardo
    edit segundo Filipe
    edit terceiro 'Soares'
    edit resto 'Garcia da Silva'
    family main
        repeat date YMD 20090101 20101231
        limit simultaneous_tasks 5
        limit outro_lim 10
        edit var5 'Esta e longa'
        task teste
            label information "nada de mais"
            meter progress 0 100 100
            edit var1 Ricardo
            edit var2 Joao
        endtask
        task outra
            edit var3 Perdigao
        endtask
        task maisuma
            edit var4 Sandra
        endtask
        family subfam1
            task dentro
                trigger ((../outra == complete OR ../outra == unknown) AND (../maisuma == complete OR ../maisuma == unknown))
                inlimit /main:simultaneous_tasks
                edit var6 Uma
                edit var7 Duas
            endtask
        endfamily
    endfamily
    family lag
        edit host geo2
        task t4
            edit v1 23
    endfamily
endsuite
